page {
  font-family: "黑体", "宋体";
}

.title {
  color: white;
  font-size: 20px;
  text-align: center;
  line-height: 30px;
  background-color: orange;
  box-shadow: 2px 2px 10px #b8b8b8;
  margin-bottom: 10px;
}

.desc {
  color: white;
}

.time {
  height: 160px;
}

.big-circle {
  width: 150px;
  height: 150px;
  position: absolute;
  border-radius: 50%;
}

.circle {
  width: 130px;
  height: 130px;
  position: absolute;
  border-radius: 50%;
  margin-left: 10px;
  margin-top: 10px;
}

.circle view{
  margin-left: 55px;
  padding-top: 55px;
  color: black;
  font-size: 20px;
}

.right {
  clip: rect(0, auto, auto, 75px);
}

.first{
  left:20rpx;
}

.second{
  left:390rpx;
}

.blue {
  background: #9cf;
}

.blue1 {
  background: #09f;
}

.white{
  background-color: white
}

.btnGroup {
  display: flex;
  flex-direction: row;
}

.btn {
  background-color: #09c;
  color: white;
}
